<UserControl x:Class="UCSearch.IUCSearch"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UCSearch"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <d:UserControl.DataContext>
        <local:UCSearchViewM/>
    </d:UserControl.DataContext>
    <Grid Margin="0,0,-104,0">
        <Label FontSize="30" Margin="10,10,394,140" Content="LOT" Width="100" Height="50">
        </Label>
        <TextBox x:Name="tb_lot" TextWrapping="Wrap" CharacterCasing="Upper" Width="180" Height="50" Margin="88,10,236,140" Text="{Binding StrLot}"/>
        <Button Margin="284,10,100,140" Content="검색" Command="{Binding ButtonCommand}" Width="120" Height="50"/>
    </Grid>
</UserControl>
